{
  "name": "file-focus",
  "displayName": "File Focus",
  "icon": "images/icon.PNG",
  "description": "Reorganize your project into virtual folders. Organize files manually or with glob patterns, open groups with one click, and more.",
  "version": "1.5.11",
  "publisher": "herdingbits",
  "repository": "https://github.com/quicken/filefocus",
  "engines": {
    "vscode": "^1.84.2"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "dynamic folders",
    "group files",
    "focus group",
    "active folders"
  ],
  "galleryBanner": {
    "color": "#414F6A",
    "theme": "dark"
  },
  "preview": false,
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fileFocusTree.refreshEntry",
        "title": "Refresh",
        "icon": "$(extensions-refresh)"
      },
      {
        "command": "fileFocusExtension.addGroup",
        "title": "Add group",
        "icon": "$(new-folder)"
      },
      {
        "command": "fileFocusExtension.pinGroup",
        "title": "Pin group",
        "icon": "$(pin)"
      },
      {
        "command": "fileFocusExtension.openGroup",
        "title": "Open all files",
        "icon": "$(multiple-windows)"
      },
      {
        "command": "fileFocusExtension.removeGroup",
        "title": "Delete group",
        "icon": "$(trash)"
      },
      {
        "command": "fileFocusExtension.renameGroup",
        "title": "Rename group",
        "icon": "$(edit)"
      },
      {
        "command": "fileFocusExtension.addGroupResource",
        "title": "Add to focus group",
        "icon": "$(tag-add)"
      },
      {
        "command": "fileFocusExtension.removeGroupResource",
        "title": "Remove from group",
        "icon": "$(remove-close)"
      },
      {
        "command": "fileFocusExtension.resetStorage",
        "title": "Reset storage",
        "icon": "$(remove-close)"
      },
      {
        "command": "fileFocusExtension.reloadStorage",
        "title": "Reload",
        "icon": "$(extensions-refresh)"
      },
      {
        "command": "fileFocusExtension.renameFileObject",
        "title": "Rename",
        "icon": "$(edit)"
      },
      {
        "command": "fileFocusExtension.createFileFolder",
        "title": "New folder...",
        "icon": "$(new-folder)"
      },
      {
        "command": "fileFocusExtension.createFile",
        "title": "New file...",
        "icon": "$(new-file)"
      }
    ],
    "configuration": [
      {
        "title": "File Focus",
        "properties": {
          "filefocus.useGlobalStorage": {
            "type": "boolean",
            "default": false,
            "description": "Store groups in vscode global storage. Allows syncing groups with vscode accounts."
          },
          "filefocus.sortkey": {
            "type": "string",
            "default": "basename",
            "description": "Group files by filename or by path.",
            "enum": [
              "basename",
              "path"
            ],
            "enumDescriptions": [
              "Group by name",
              "Group by path"
            ]
          },
          "filefocus.showProjectGroups": {
            "type": "boolean",
            "default": true,
            "description": "Disable to hide all groups defined inside .filefocus.json files."
          },
          "filefocus.showExcludedGroup": {
            "type": "boolean",
            "default": false,
            "description": "Show a group containing all files matching vscodes files.exclude file. (experimental)"
          },
          "filefocus.addToPinnedGroupOnOpen": {
            "type": "boolean",
            "default": false,
            "description": "Automatically add opened files to the currently pinned group."
          },
          "filefocus.globgroup": {
            "type": "array",
            "default": [],
            "description": "Defines dynamic groups based on glob patterns. See project readme/home page for details."
          }
        }
      }
    ],
    "keybindings": [
      {
        "command": "fileFocusExtension.addGroupResource",
        "key": "ctrl+shift+8",
        "mac": "cmd+shift+8",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "fileFocusExtension.addGroupResource",
          "group": "z_commands"
        }
      ],
      "editor/title/context": [
        {
          "command": "fileFocusExtension.addGroupResource",
          "group": "z_commands"
        }
      ],
      "explorer/context": [
        {
          "command": "fileFocusExtension.addGroupResource",
          "group": "z_commands"
        }
      ],
      "view/item/context": [
        {
          "command": "fileFocusExtension.openGroup",
          "when": "view == fileFocusTree && viewItem =~ /GroupItem/",
          "group": "inline@3"
        },
        {
          "command": "fileFocusExtension.pinGroup",
          "when": "view == fileFocusTree && viewItem == GroupItem",
          "group": "inline@4"
        },
        {
          "command": "fileFocusExtension.renameGroup",
          "when": "view == fileFocusTree && viewItem == GroupItem",
          "group": "inline@2"
        },
        {
          "command": "fileFocusExtension.removeGroup",
          "when": "view == fileFocusTree && viewItem == GroupItem",
          "group": "inline@1"
        },
        {
          "command": "fileFocusExtension.removeGroupResource",
          "when": "view == fileFocusTree && viewItem =~ /Root/ && viewItem =~ /Write/",
          "group": "inline"
        },
        {
          "command": "fileFocusExtension.renameFileObject",
          "when": "view == fileFocusTree && (viewItem =~ /FocusFile/ || viewItem =~ /FocusFolder/) && viewItem =~ /Write/"
        },
        {
          "command": "fileFocusExtension.createFileFolder",
          "when": "view == fileFocusTree && viewItem =~ /FocusFolder/ && viewItem =~ /Write/"
        },
        {
          "command": "fileFocusExtension.createFile",
          "when": "view == fileFocusTree && viewItem =~ /FocusFolder/ && viewItem =~ /Write/"
        }
      ],
      "view/title": [
        {
          "command": "fileFocusExtension.addGroup",
          "when": "view == fileFocusTree",
          "group": "navigation"
        },
        {
          "command": "fileFocusExtension.reloadStorage",
          "when": "view == fileFocusTree",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "fileFocusTree",
          "name": "File Focus",
          "icon": "images/sideicon.png",
          "contextualTitle": "File Focus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/simple-mock": "^0.8.6",
    "@types/uuid": "^9.0.7",
    "@types/vscode": "^1.84.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "simple-mock": "^0.8.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@humanwhocodes/gitignore-to-minimatch": "^1.0.2",
    "minimatch": "^9.0.3",
    "uuid": "^9.0.1",
    "vscode-uri": "^3.0.8"
  },
  "__metadata": {
    "id": "fbd58705-7727-43e2-8702-adc9e22740b3",
    "publisherDisplayName": "Herding Bits - M. Scherzer",
    "publisherId": "8e5ec25c-4336-4325-9b09-31cfa9bf5f02",
    "isPreReleaseVersion": false
  }
}
